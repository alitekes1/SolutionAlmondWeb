@model  List<ProfileTable>
<link href="~/Content/AlmondCss/AllProfile.css" rel="stylesheet" />
<div class="row gx-0" id="resultUserCards">
    @foreach (var item in Model)
    {
        <div class="col-sm-3 my-5">
            <div class="card">
                <div class="box">
                    <div class="img">
                        <img src="@Url.Content(item.profileImageUrl)" alt="imageNotUpload" width="150">
                    </div>
                    <h2>@@@item.Owner.Username<br><span>@item.job</span></h2>
                    <p>
                        @item.aboutmeText
                    </p>
                    <span>
